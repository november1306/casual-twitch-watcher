  "types": "dist/src/index.d.ts",
  "keywords": [
    "automation",
    "stealth",
    "bot-detection",
    "webgl-spoofing",
    "fingerprinting",
    "playwright",
    "patchright",
    "anti-detection"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
